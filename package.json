    "jasmine": "2.8.0",
    "jshint": "2.9.5",
    "ovh-iconlib-provider-svg-cleaner-svgo": "^0.2.0"
  },
  "dependencies": {
    "@rduk/logger": "^0.1.5",
    "ovh-iconlib-provider-storage": "^0.3.0",
    "ovh-iconlib-provider-svg-cleaner": "^0.1.2",
    "request-promise": "^4.2.2"
  },
  "config": {
